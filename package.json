{
  "productName": "PeptideStackViz Electron App",
  "main": "electron/main.js",
  "homepage": "./",
  "name": "peptide_bioactive_viz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.4.0",
    "@types/d3-contour": "^3.0.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "canvg": "^4.0.1",
    "d3": "^7.8.2",
    "htl": "^0.3.1",
    "jspdf": "^2.5.1",
    "papaparse": "^5.3.2",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "svg-to-pdfkit": "^0.1.8",
    "svg2pdf.js": "^2.2.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "wkhtmltopdf": "^0.4.0"
  },
  "scripts": {
    "build-electron-unix": "mkdir build/src && cp -r electron/. build/electron",
    "build-electron-windows": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "start": "set BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/svg-to-pdfkit": "^0.1.0",
    "@types/wkhtmltopdf": "^0.3.4",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-cra-example",
      "owner": "johndyer24"
    }
  },
  "browser": {
    "child_process": false
  }
}
